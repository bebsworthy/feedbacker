{
  "name": "@feedbacker/core",
  "version": "1.0.0",
  "type": "module",
  "description": "A drop-in React feedback system for component-level feedback capture during development and design review",
  "keywords": ["react", "feedback", "component", "development", "ui", "ux"],
  "author": "Feedbacker Team",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/feedbacker.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "dev": "npm run build:watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "cd demo && npm start",
    "demo:build": "cd demo && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.4",
    "rollup": "^3.29.2",
    "rollup-plugin-postcss": "^4.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "html2canvas": "^1.4.1"
  },
  "peerDependenciesMeta": {
    "html2canvas": {
      "optional": true
    }
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feedbacker/core.git"
  },
  "bugs": {
    "url": "https://github.com/feedbacker/core/issues"
  },
  "homepage": "https://github.com/feedbacker/core#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}